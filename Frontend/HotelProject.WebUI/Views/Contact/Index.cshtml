
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("Head")

<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        @await Component.InvokeAsync("Spinner")
        <!-- Spinner End -->
        <!-- Header Start -->
        @await Component.InvokeAsync("Navbar")
        <!-- Header End -->
        <!-- Page Header Start -->
        @await Component.InvokeAsync("ContactHeader")
        <!-- Page Header End -->
        <!-- Booking Start -->
        @* @await Component.InvokeAsync("Reservation") *@
        <!-- Booking End -->
        <!-- Contact Start -->
        @await Html.PartialAsync("/Views/Contact/SendMessage.cshtml")
        <!-- Contact End -->
        <!-- Newsletter Start -->
        @await Html.PartialAsync("/Views/Default/SubscribePartial.cshtml")
        <!-- Newsletter Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("Footer")
        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    @await Component.InvokeAsync("Scripts")
</body>

</html>
